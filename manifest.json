{
  "manifest_version": 2,
  "name": "Cnrtl",
  "version": "1.0",
  "description": "Get definition, synonyms, etymology of selected words with CNRTL website",
  "icons": {
    "48": "icons/icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage",
    "tabs"
  ]
}
